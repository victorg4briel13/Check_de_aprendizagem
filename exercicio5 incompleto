#include <stdio.h>
#include <stdlib.h>

int main(){
  
  int n1, n2, nm3;
  
  printf("Escreva o primeiro valor");
  scanf("%d", &num1);
  
  printf("Digite o segundo valor");
  scanf("%d", &num2);
  
  printf("Digite o terceiro valor");
  scanf("%d", &num3);

  if(n1<n2 && n1<n3){
    printf("\n %d \n", n1);
    if (n2<n3){
      printf("%d%d \n", n2,n3);
    }else{
      printf("%d%d", n3, n2);
    }
  }
  
	return 0;
}
