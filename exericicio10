#include <stdio.h>
#include <stdlib.h>

int main(){
  
  int l1, l2, l3;
  
  printf("Digite a medida do primeiro lado: ");
  scanf("%d", &l1);
  
  printf("Digite a medida do segundo lado: ");
  scanf("%d", &l2);
  
  printf("Digite a medida do terceiro lado: ");
  scanf("%d", &l3);

  if(l1 == l2 && l1 == l3){
    printf("\n Triangulo equilatero");
  }else if(l1 == l2 || l1 == l3 || l2 == l3){
    printf("\n Triangulo isoseles");
  }else{
    printf("Trinagulo escaleno");
  }
  
	return 0;
}
